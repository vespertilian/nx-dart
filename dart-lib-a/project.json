{
  "name": "dart-lib-a",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "dart-lib-a",
  "projectType": "library",
  "targets": {
    "list": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "dart-lib-a",
        "command": "ls lib"
      }
    },
    "dart-test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "dart-lib-a",
        "command": "dart test -p chrome"
      },
      "cache": true
   },
    "pub-get": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "dart-lib-a",
        "command": "dart pub get"
      }
    }
  }
}
